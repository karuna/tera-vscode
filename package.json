{
  "name": "tera",
  "displayName": "Tera",
  "description": "A powerful, easy-to-use template language based on Rust, inspired by Jinja2 and Django Templates.",
  "version": "0.1.0",
  "publisher": "karunamurti",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "license": "MIT",
  "keywords": [
    "tera",
    "rust"
  ],
  "main": "./lib/index.js",
  "contributes": {
    "languages": [
      {
        "id": "tera-html",
        "aliases": [
          "Tera HTML",
          "tera-html"
        ],
        "extensions": [
          ".tera",
          ".html.tera"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "tera",
        "aliases": [
          "Tera Raw",
          "tera-raw"
        ],
        "extensions": [],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tera-html",
        "scopeName": "text.html.tera",
        "path": "./syntaxes/tera-html.json"
      },
      {
        "language": "tera",
        "scopeName": "source.tera",
        "path": "./syntaxes/tera.json"
      }
    ],
    "snippets": [
      {
        "language": "tera-html",
        "path": "./snippets/tera.code-snippets.json"
      }
    ],
    "configurationDefaults": {
      "[tera-html]": {
        "editor.quickSuggestions": {
          "other": true,
          "comments": true,
          "strings": true
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karuna/tera-vscode.git"
  },
  "galleryBanner": {
    "color": "#888888",
    "theme": "dark"
  },
  "homepage": "https://github.com/karuna/tera-vscode",
  "bugs": {
    "url": "https://github.com/karuna/tera-vscode/issues",
    "email": "karuna.murti@gmail.com"
  },
  "icon": "images/logo.png",
  "activationEvents": [
    "onLanguage:tera"
  ],
  "scripts": {},
  "dependencies": {
    "prettydiff": "^101.2.6"
  },
  "devDependencies": {
    "@types/vscode": "^1.40.0"
  }
}
